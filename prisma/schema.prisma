
generator client {
    provider = "prisma-client-js"
    binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
    output = "/home/ubuntu/veritavo_ai_website/nextjs_space/node_modules/.prisma/client"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model ContactSubmission {
  id          String   @id @default(cuid())
  name        String
  email       String
  company     String?
  phone       String?
  message     String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model WebsiteScanRequest {
  id          String   @id @default(cuid())
  name        String
  email       String
  company     String?
  websiteUrl  String
  phone       String?
  message     String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
